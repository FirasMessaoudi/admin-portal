export declare class DccCommonsNgServicesModule {
    constructor(parentModule: DccCommonsNgServicesModule);
    static forRoot(publicUrlPatterns: Array<string>, loginUrl: string, i18nFolderPath: string): ModuleWithProviders;
}
export declare function HttpLoaderFactory(http: HttpClient, i18nFolderPath: string): TranslateHttpLoader;
