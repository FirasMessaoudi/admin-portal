import { HttpEvent, HttpHandler, HttpInterceptor, HttpRequest } from '@angular/common/http';
import { Observable } from 'rxjs';
import { CookieService } from "ngx-cookie-service";
import { Router } from "@angular/router";
export declare const PUBLIC_URL_PATTERNS: string;
export declare const LOGIN_URL: string;
export declare class JwtInterceptor implements HttpInterceptor {
    private cookieService;
    private router;
    private environment;
    private publicUrlPatterns;
    private loginUrl;
    tokenExpiryDateTimerSubscription: any;
    constructor(cookieService: CookieService, router: Router, environment: any, publicUrlPatterns?: Array<String>, loginUrl?: string);
    intercept(request: HttpRequest<any>, next: HttpHandler): Observable<HttpEvent<any>>;
    refreshTokenTimer(): void;
}
